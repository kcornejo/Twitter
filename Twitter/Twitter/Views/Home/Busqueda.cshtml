
@{
    ViewBag.Title = "Home Page";
    Layout = "";
}

<table class="table">


    @for (int i = 0; i < 10; i++)
    {
        if (Model[i, 0] != null)
        {
            <tr onclick="location.href='@Url.Action("PerfilExterno", "Home", new{ username = Model[i,0] })'">
                <td><img width="40px" height="40px" class="img-circle" src="~/Content/img/@Model[i,2]" alt="user image"></td>
                <td>
                    @Model[i, 1] (@Model[i, 0])
                </td>
            </tr>
        }
    }
</table>